              "& when (@showCardsCategory), (@showCardsDate)": {
                ".card-category, .card-date": {
                  "&:before": {
                    "content": "''",
                    "display": "inline-block",
                    "background-color": "@warningColor"
                  },
                  "& when (@showEffectLinePost)": {
                    "&:before": {
                      "-webkit-transition": "all .2s ease",
                      "transition": "all .2s ease"
                    },
                    "&:hover:before": {
                      "transform": "rotate(90deg)",
                      "background-color": "@primaryColor"
                    }
                  }
                },
                ".card-category:before when (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                },
                ".card-date:before when (@showCardsCategory), (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                }
              }
            },
            "@media (min-width: 768px)": {
              "& when (@showLineText)": {
                "& when (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                  ".card-text-box, .category-wrap": {
                    "padding-left": "50px"
                  },
                  ".card-text-box": {
                    "position": "relative",
                    "&:before": {
                      "content": "\"\"",
                      "position": "absolute",
                      "top": "14px",
                      "left": "0",
                      "width": "30px",
                      "height": "3px",
                      "background-color": "@primaryColor"
                    }
                  }
                }
              }
            },
            ".title when (@showMilestone) and not (@titleAlign = '2')": {
              ".milestone > div > div": {
                "padding-left": "0",
                "padding-right": "0"
              },
              ".milestone, .milestone > div, .milestone > div > div": {
                "width": "100%"
              },
              ".milestone > div > div when (@showBorderBottom)": {
                "border-bottom": "1px solid @milBorderColor"
              },
              ".milestone .mbr-text": {
                "padding-top": "5px"
              },
              ".milestone > div when (@showTitle), (@showSubtitle)": {
                "& when (@milCount > 2)": {
                  "&:nth-child(1), &:nth-child(2)": {
                    "padding-bottom": "20px"
                  }
                }
              },
              "@media (max-width: 767px)": {
                ".title-wrap": {
                  "padding-bottom": "20px"
                },
                ".milestone > div:not(:last-child)": {
                  "padding-bottom": "20px"
                },
                ".milestone .numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-box, .card-box > *:not(.mbr-overlay)": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img when (@showImgOverlay) and (@showImg)": {
              "amp-img": {
                "position": "relative",
                "&:before": {
                  "content": "\"\"",
                  "position": "absolute",
                  "height": "100%",
                  "width": "100%",
                  "z-index": "1",
                  "pointer-events": "none",
                  "-webkit-transition": "all 0.2s ease",
                  "transition": "all 0.2s ease"
                },
                "> a:before when (@showImgButton)": {
                  "content": "\"＋\"",
                  "position": "absolute",
                  "top": "50%",
                  "left": "50%",
                  "font-size": "40px",
                  "transform": "translate(-50%, -50%)",
                  "color": "transparent",
                  "z-index": "2",
                  "-webkit-transition": "all 0.2s ease",
                  "transition": "all 0.2s ease"
                }
              },
              "&:hover": {
                "amp-img:before": {
                  "background-color": "@imgOverlayColor",
                  "opacity": "@imgOverlayOpacity"
                },
                "amp-img > a:before when (@showImgButton)": {
                  "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".milestone > div:last-child > div": {
                "border-bottom": "none"
              }
            },
            ".title-wrap .mbr-section-title": {
              "color": "#434343"
            },
            ".card-title": {
              "color": "#9c8000",
              "text-align": "left"
            },
            ".card-subtitle": {
              "color": "#595959"
            },
            ".milestone .numeral": {
              "color": "#434343"
            },
            ".milestone .mbr-text": {
              "color": "#434343"
            },
            ".card-category": {
              "color": "#595959"
            },
            ".card-info": {
              "color": "#595959"
            },
            ".card .mbr-text": {
              "color": "#222222"
            }
          },
          "_name": "features2",
          "_customHTML": "<section class=\"features2\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\" condition=\"titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle)\" checked=\"\">\n        <input type=\"checkbox\" title=\"Border Bottom\" name=\"showBorderBottom\" condition=\"showMilestone &amp;&amp; (titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle))\" checked=\"\">\n        <input type=\"color\" title=\"Border Color\" name=\"milBorderColor\" value=\"#bec1c7\" condition=\"showMilestone &amp;&amp; showBorderBottom &amp;&amp; (titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle))\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Count Milestone\" name=\"milCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showMilestone &amp;&amp; (titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle))\">\n        <input type=\"checkbox\" title=\"Reverse Content Title\" name=\"reverseContent\" condition=\"showMilestone &amp;&amp; (showTitle || showSubtitle) &amp;&amp; titleAlign != 2\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        \n        <header>Cards</header>\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n        <input type=\"checkbox\" title=\"Show Post Info\" name=\"showCardsInfo\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Post Category\" name=\"showCardsCategory\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Post Date\" name=\"showCardsDate\">\n        <input type=\"checkbox\" title=\"Show Line Info Post\" name=\"showLinePost\" condition=\"(showCardsInfo &amp;&amp; showCardsCategory) || (showCardsInfo &amp;&amp; showCardsDate) || (showCardsCategory &amp;&amp; showCardsDate)\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Effect Post\" name=\"showEffectLinePost\" condition=\"showLinePost &amp;&amp; ((showCardsInfo &amp;&amp; showCardsCategory) || (showCardsInfo &amp;&amp; showCardsDate) || (showCardsCategory &amp;&amp; showCardsDate))\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\">\n        <input type=\"checkbox\" title=\"Show line Text\" name=\"showLineText\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" checked=\"\">\n        <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Image Overlay\" name=\"showImgOverlay\" condition=\"showImg\" checked=\"\">\n        <input type=\"color\" title=\"Image Overlay Color\" name=\"imgOverlayColor\" value=\"#947937\" condition=\"showImg &amp;&amp; showImgOverlay\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showImg &amp;&amp; showImgOverlay\">\n        <input type=\"checkbox\" title=\"Show Image Button\" name=\"showImgButton\" condition=\"showImg &amp;&amp; showImgOverlay\" checked=\"\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"!transparentBg &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n        \n        <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background72.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#fafafa\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#9ea1a9\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n    'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n    'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n    'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n    'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n    'container-fluid':fullWidth}\">\n    <div class=\"mbr-row title mbr-pb-4\" mbr-if=\"showTitle || showSubtitle || showMilestone\" mbr-class=\"{'mbr-row-reverse':showMilestone &amp;&amp; (showTitle || showSubtitle) &amp;&amp; titleAlign != 2 &amp;&amp; reverseContent, 'mbr-px-2': fullWidth, 'align-left': titleAlign == 1, 'align-center': titleAlign == 2, 'align-right': titleAlign == 3}\">\n        <div class=\"title-wrap\" mbr-class=\"{'mbr-col-sm-12 mbr-col-md-4': showMilestone &amp;&amp; titleAlign != 2, 'mbr-col-sm-12 mbr-col-md-12':!showMilestone || titleAlign == 2}\" mbr-if=\"showTitle || showSubtitle\">        \n            <h2 class=\"mbr-section-title mbr-semibold mbr-fonts-style\" mbr-class=\"{'mbr-pb-2':showSubtitle}\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\"><span style=\"font-weight: normal;\">Pakketilbud</span></h2>\n            <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">\n                Features with Images Subtitle\n            </h3>\n        </div>\n        <div class=\"milestone mbr-flex flex-wrap\" mbr-if=\"showMilestone &amp;&amp; titleAlign != 2\" mbr-list=\"\" mbr-list-grow=\"milCount\" mbr-class=\"{'mbr-col-sm-12 mbr-col-md-8': showTitle || showSubtitle}\">\n            <div mbr-class=\"{'mbr-col-xl-4':milCount == 3 &amp;&amp; fullWidth, 'mbr-col-md-6 mbr-col-lg-6': showTitle || showSubtitle, 'mbr-col-sm-12 mbr-col-md-3': !showTitle &amp;&amp; !showSubtitle}\">\n                <div class=\"mbr-m-auto\" mbr-class=\"{'mbr-pb-4': showBorderBottom, 'mbr-col-md-8':showTitle || showSubtitle, 'mbr-col-md-12 mbr-col-lg-8':!showTitle &amp;&amp; !showSubtitle}\">\n                    <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-e': titleAlign == 3}\">\n                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".milestone .numeral\">\n                            2</h4>\n                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".milestone .numeral\"><span style=\"font-weight: normal;\">&nbsp;måneder</span></h4>\n                    </div>\n                    <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".milestone .mbr-text\">Til du garantert vil se resultat</p>\n             </div>\n         </div>\n         <div mbr-class=\"{'mbr-col-xl-4':milCount == 3 &amp;&amp; fullWidth, 'mbr-col-md-6 mbr-col-lg-6': showTitle || showSubtitle, 'mbr-col-sm-12 mbr-col-md-3': !showTitle &amp;&amp; !showSubtitle}\">\n                <div class=\"mbr-m-auto\" mbr-class=\"{'mbr-pb-4': showBorderBottom, 'mbr-col-md-8':showTitle || showSubtitle, 'mbr-col-md-12 mbr-col-lg-8':!showTitle &amp;&amp; !showSubtitle}\">\n                    <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-e': titleAlign == 3}\">\n                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".milestone .numeral\">\n                            12</h4>\n                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".milestone .numeral\"><span style=\"font-weight: normal;\">&nbsp;timer</span></h4>\n                    </div>\n                    <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-4\" data-app-selector=\".milestone .mbr-text\">Investert fra din side</p>\n             </div>\n         </div></div>\n </div>\n <div class=\"mbr-row card-container mbr-jc-c\" mbr-if=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\" mbr-class=\"{'mbr-px-2': fullWidth &amp;&amp;  cardsAmount != 1, 'cardsAmount2': cardsAmount == 2}\">\n    <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12 last-child': cardsAmount == 1,\n    'mbr-col-md-6 mbr-col-xl-6': cardsAmount == 2,\n    'mbr-col-md-4': cardsAmount == 3,\n    'mbr-col-lg-3 mbr-col-md-6 md-pb': cardsAmount == 4}\">\n    <div class=\"card-wrapper\">\n        <div class=\"card-img\" mbr-if=\"showImg\" mbr-class=\"{'img-with-overlay': showImgOverlay}\">\n            <amp-img src=\"@PROJECT_PATH@/assets/images/dsc01457-1110x623.jpg\" layout=\"responsive\" width=\"555.8908507223114\" class=\"mobirise-loader\" height=\"312\" alt=\"Mobirise\">\n                <div placeholder=\"\" class=\"placeholder\"></div>\n                <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/dsc01457-1110x623.jpg\" alt=\"Mobirise\" title=\"\">\n            </amp-img>\n        </div>\n        <div class=\"card-box\" mbr-if=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'mbr-pt-4': showImg, 'mbr-p-2': showBorders || !transparentBg, 'mbr-col-sm-12': fullWidth}\">\n            <div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n            <div class=\"category-wrap\" mbr-if=\"showCardsInfo || showCardsCategory || showCardsDate\" mbr-class=\"{'mbr-pb-3':showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons}\">\n                <p class=\"card-info mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsInfo\" data-app-selector=\".card-info\">\n                Pakke 1</p>\n                <p class=\"card-category mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsCategory\" data-app-selector=\".card-category\">Myk yoga, meditasjon &amp; avspenning</p>\n                <p class=\"card-date mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsDate\" data-app-selector=\".card-date\">\n                June 28, 2018</p>\n            </div>\n            <div class=\"card-text-box\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\" mbr-if=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n                <h3 class=\"card-title mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-2\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pb-2':showCardsText || showCardsSubtitle}\"><span style=\"font-weight: normal;\">Stress Management</span></h3>\n                <h4 class=\"card-subtitle mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsSubtitle\" data-app-selector=\".card-subtitle\" mbr-class=\"{'mbr-pb-2':showCardsText}\"><b>- Seminar om stresshåndtering</b><div><b>- Inspirerende meditasjonsworkshop</b></div><div><b>- 2 måneder med Myk Yoga</b></div></h4>\n                <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showCardsText\" data-app-selector=\".card .mbr-text\">Nordmenn ligger på Europa-toppen i jobbstress. 60% av fravær fra jobb er forårsaket av mental uhelse og muskel- og skjelettsykdommer, ofte relatert til stress. Yoga er en naturlig og effektiv måte å frigjøre psykisk og fysisk stress, hvilket vil fremme god søvn og styrke konsentrasjonen. Gjennom yoga og meditasjonsteknikker lærer vi dine ansatte hvordan de kan opprettholde en langsiktig motstandskraft mot stress. Dette tilbudet er skreddersydd for en høypresterende arbeidsplass.<br></p>\n                <div class=\"card-btn mbr-section-btn\" mbr-buttons=\"\" data-toolbar=\"-mbrBtnMove\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsButtons\" mbr-class=\"{'mbr-pt-3':showCardsText || showCardsSubtitle || showCardsTitle}\"><a class=\"btn btn-md btn-info\" href=\"popup:#amp-popup-e\" data-app-placeholder=\"Type Text\" on=\"tap:amp-popup-e\">\n                Få tilbud</a></div>\n        </div>\n    </div>\n</div>\n</div>\n\n<div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6 last-child': cardsAmount == 2,\n'mbr-col-md-4': cardsAmount == 3,\n'mbr-col-lg-3 mbr-col-md-6 md-pb': cardsAmount == 4}\" mbr-if=\"cardsAmount > 1\">\n<div class=\"card-wrapper\">\n    <div class=\"card-img\" mbr-if=\"showImg\" mbr-class=\"{'img-with-overlay': showImgOverlay}\">\n        <amp-img src=\"@PROJECT_PATH@/assets/images/dsc00300-1110x623.jpg\" layout=\"responsive\" width=\"555.8908507223114\" class=\"mobirise-loader\" height=\"312\" alt=\"Mobirise\">\n            <div placeholder=\"\" class=\"placeholder\"></div>\n            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/dsc00300-1110x623.jpg\" alt=\"Mobirise\" title=\"\">\n        </amp-img>\n    </div>\n    <div class=\"card-box\" mbr-if=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\" mbr-class=\"{'mbr-pt-4': showImg, 'mbr-p-2': showBorders || !transparentBg, 'mbr-col-sm-12': fullWidth}\">\n        <div class=\"mbr-overlay\" mbr-if=\"!transparentBg\"></div>\n        <div class=\"card-title-box mbr-flex flex-wrap\" mbr-if=\"showCardsInfo || showCardsCategory || showCardsDate\" mbr-class=\"{'mbr-pb-3':showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons}\">\n            <div class=\"category-wrap\" mbr-if=\"showCardsInfo || showCardsCategory || showCardsDate\">\n                <p class=\"card-info mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsInfo\" data-app-selector=\".card-info\">\n                Pakke 2</p>\n                <p class=\"card-category mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsCategory\" data-app-selector=\".card-category\">Yoga Boost, mindfulness og motivasjon</p>\n                <p class=\"card-date mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showCardsDate\" data-app-selector=\".card-date\">\n                June 28, 2018</p>\n            </div>\n        </div>\n        <div class=\"card-text-box\" mbr-class=\"{'align-center': cardsAlign == 2, 'align-right': cardsAlign == 3}\" mbr-if=\"showCards