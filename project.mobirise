        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".category-wrap > * when (@showCardsCategory), (@showCardsDate), (@showCardsInfo)": {
              "display": "inline-block",
              "padding-right": "14px"
            },
            ".category-wrap when (@showLinePost)": {
              "& when (@showCardsCategory), (@showCardsDate)": {
                ".card-category, .card-date": {
                  "&:before": {
                    "content": "''",
                    "display": "inline-block",
                    "background-color": "@warningColor"
                  },
                  "& when (@showEffectLinePost)": {
                    "&:before": {
                      "-webkit-transition": "all .2s ease",
                      "transition": "all .2s ease"
                    },
                    "&:hover:before": {
                      "transform": "rotate(90deg)",
                      "background-color": "@primaryColor"
                    }
                  }
                },
                ".card-category:before when (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                },
                ".card-date:before when (@showCardsCategory), (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                }
              }
            },
            "@media (min-width: 768px)": {
              "& when (@showLineText)": {
                "& when (@showCardsTitle), (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                  ".card-text-box, .category-wrap": {
                    "padding-left": "50px"
                  },
                  ".card-text-box": {
                    "position": "relative",
                    "&:before": {
                      "content": "\"\"",
                      "position": "absolute",
                      "top": "14px",
                      "left": "0",
                      "width": "30px",
                      "height": "3px",
                      "background-color": "@primaryColor"
                    }
                  }
                }
              }
            },
            ".title when (@showMilestone) and not (@titleAlign = '2')": {
              ".milestone > div > div": {
                "padding-left": "0",
                "padding-right": "0"
              },
              ".milestone, .milestone > div, .milestone > div > div": {
                "width": "100%"
              },
              ".milestone > div > div when (@showBorderBottom)": {
                "border-bottom": "1px solid @milBorderColor"
              },
              ".milestone .mbr-text": {
                "padding-top": "5px"
              },
              ".milestone > div when (@showTitle), (@showSubtitle)": {
                "& when (@milCount > 2)": {
                  "&:nth-child(1), &:nth-child(2)": {
                    "padding-bottom": "20px"
                  }
                }
              },
              "@media (max-width: 767px)": {
                ".title-wrap": {
                  "padding-bottom": "20px"
                },
                ".milestone > div:not(:last-child)": {
                  "padding-bottom": "20px"
                },
                ".milestone .numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-box, .card-box > *:not(.mbr-overlay)": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img when (@showImgOverlay) and (@showImg)": {
              "amp-img": {
                "position": "relative",
                "&:before": {
                  "content": "\"\"",
                  "position": "absolute",
                  "height": "100%",
                  "width": "100%",
                  "z-index": "1",
                  "pointer-events": "none",
                  "-webkit-transition": "all 0.2s ease",
                  "transition": "all 0.2s ease"
                },
                "> a:before when (@showImgButton)": {
                  "content": "\"＋\"",
                  "position": "absolute",
                  "top": "50%",
                  "left": "50%",
                  "font-size": "40px",
                  "transform": "translate(-50%, -50%)",
                  "color": "transparent",
                  "z-index": "2",
                  "-webkit-transition": "all 0.2s ease",
                  "transition": "all 0.2s ease"
                }
              },
              "&:hover": {
                "amp-img:before": {
                  "background-color": "@imgOverlayColor",
                  "opacity": "@imgOverlayOpacity"
                },
                "amp-img > a:before when (@showImgButton)": {
                  "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".milestone > div:last-child > div": {
                "border-bottom": "none"
              }
            }
          },
          "_name": "features2",
          "_customHTML": "<section class=\"features2\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\" condition=\"titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle)\">\n        <input type=\"checkbox\" title=\"Border Bottom\" name=\"showBorderBottom\" condition=\"showMilestone &amp;&amp; (titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle))\" checked=\"\">\n        <input type=\"color\" title=\"Border Color\" name=\"milBorderColor\" value=\"#bec1c7\" condition=\"showMilestone &amp;&amp; showBorderBottom &amp;&amp; (titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle))\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Count Milestone\" name=\"milCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showMilestone &amp;&amp; (titleAlign != 2 || (!showTitle &amp;&amp; !showSubtitle))\">\n        <input type=\"checkbox\" title=\"Reverse Content Title\" name=\"reverseContent\" condition=\"showMilestone &amp;&amp; (showTitle || showSubtitle) &amp;&amp; titleAlign != 2\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        \n        <header>Cards</header>\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n        <input type=\"checkbox\" title=\"Show Post Info\" name=\"showCardsInfo\">\n        <input type=\"checkbox\" title=\"Show Post Category\" name=\"showCardsCategory\">\n        <input type=\"checkbox\" title=\"Show Post Date\" name=\"showCardsDate\">\n        <input type=\"checkbox\" title=\"Show Line Info Post\" name=\"showLinePost\" condition=\"(showCardsInfo &amp;&amp; showCardsCategory) || (showCardsInfo &amp;&amp; showCardsDate) || (showCardsCategory &amp;&amp; showCardsDate)\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Effect Post\" name=\"showEffectLinePost\" condition=\"showLinePost &amp;&amp; ((showCardsInfo &amp;&amp; showCardsCategory) || (showCardsInfo &amp;&amp; showCardsDate) || (showCardsCategory &amp;&amp; showCardsDate))\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show line Text\" name=\"showLineText\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n        <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Image Overlay\" name=\"showImgOverlay\" condition=\"showImg\" checked=\"\">\n        <input type=\"color\" title=\"Image Overlay Color\" name=\"imgOverlayColor\" value=\"#000000\" condition=\"showImg &amp;&amp; showImgOverlay\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showImg &amp;&amp; showImgOverlay\">\n        <input type=\"checkbox\" title=\"Show Image Button\" name=\"showImgButton\" condition=\"showImg &amp;&amp; showImgOverlay\" checked=\"\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"!transparentBg &amp;&amp; (showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\